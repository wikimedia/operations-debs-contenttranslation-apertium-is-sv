<!--
	SWEDISH PASSIVE VOICE  =======> VERÐUR PP.M.SG       levereras ===> verður afhentur
	does not work for verbs governing dat and gen where the "subject" is in the oblique case and the verb is in pp.nt.sg
-->

<rule comment="regla: V_PASS">
  <pattern>
    <pattern-item n="verb_passive"/>
  </pattern>
  <action>
      <call-macro n="firstWord">                           
        <with-param pos="1"/>
      </call-macro>    
      <choose>
	        <when>
	           <test>
	             <equal>
	               <clip pos="1" side="sl" part="temps"/>
	               <lit-tag v="inf"/>
	             </equal>
	           </test>
		     				<out>
						         <chunk name="VERB_PASSIVE_INFINITIVE" case="caseFirstWord">    <!--VERB_INF-->
						           <tags>
						             <tag><lit-tag v="VERB_PASS_INF"/></tag>     <!--V_INF-->
						              <tag><lit-tag v="vblex"/></tag>   
						              <tag><lit-tag v="actv"/></tag>   
						              <tag><lit-tag v="inf"/></tag>   
						            </tags>
						            <lu>
						              <lit v="verða"/>
						              <lit-tag v="vblex"/>
						              <lit-tag v="actv"/>
						              <lit-tag v="inf"/>
						            </lu>
						            <b pos="0"/>
						            <lu>
						              <clip pos="1" side="tl" part="lemh"/>
						              <lit-tag v="vblex"/>
						              <lit-tag v="actv"/>
									  <lit-tag v="pp"/>
				    				  <lit-tag v="m"/>
				    				  <lit-tag v="sg"/>
				    				  <lit-tag v="nom"/>
						              <lit-tag v="ind"/>        
						              <clip pos="1" side="tl" part="lemq"/>
						            </lu>
						          </chunk>
	 						</out>
        	</when>

	        <when>
	           <test>
	             <equal>
	               <clip pos="1" side="sl" part="temps"/>
	               <lit-tag v="supn"/>
	             </equal>
	           </test>
		     				<out>
						         <chunk name="VERB_PASSIVE_SUPINUM" case="caseFirstWord">    
						           <tags>
						             <tag><lit-tag v="VERB_PASS_SUPN"/></tag>     
						              <tag><lit-tag v="vblex"/></tag>   
						              <tag><lit-tag v="actv"/></tag>   
						              <tag><lit-tag v="supn"/></tag>   
						            </tags>
						            <lu>
						              <lit v="verða"/>
						              <lit-tag v="vblex"/>
						              <lit-tag v="actv"/>
						              <lit-tag v="supn"/>
						            </lu>
						            <b pos="0"/>
						            <lu>
						              <clip pos="1" side="tl" part="lemh"/>
						              <lit-tag v="vblex"/>
						              <lit-tag v="actv"/>
									  <lit-tag v="pp"/>
				    				  <lit-tag v="m"/>
				    				  <lit-tag v="sg"/>
				    				  <lit-tag v="nom"/>
						              <lit-tag v="ind"/>        
						              <clip pos="1" side="tl" part="lemq"/>
						            </lu>
						          </chunk>
	 						</out>
        	</when>

	        <when>
	           <test>
	             <equal>
	               <clip pos="1" side="sl" part="temps"/>
	               <lit-tag v="pres"/>
	             </equal>
	           </test>
		     				<out>
						         <chunk name="VERB_PASSIVE_PRESENT" case="caseFirstWord">    
						           <tags>
						             <tag><lit-tag v="VERB_PASS_PRES"/></tag>     <!-- 3p sg by default-->
						              <tag><lit-tag v="vblex"/></tag>   
						              <tag><lit-tag v="actv"/></tag>   
						              <tag><lit-tag v="pri"/></tag>   
						              <tag><lit-tag v="p3"/></tag>   
						              <tag><lit-tag v="sg"/></tag>   
						            </tags>
						            <lu>
						              <lit v="verða"/>
						              <lit-tag v="vblex"/>
						              <lit-tag v="actv"/>
						              <lit-tag v="pri"/>
						              <lit-tag v="p3"/>
						              <lit-tag v="sg"/>
						            </lu>
						            <b pos="0"/>
						            <lu>
						              <clip pos="1" side="tl" part="lemh"/>
						              <lit-tag v="vblex"/>
						              <lit-tag v="actv"/>
									  <lit-tag v="pp"/>
				    				  <lit-tag v="m"/>
				    				  <lit-tag v="sg"/>
				    				  <lit-tag v="nom"/>
						              <lit-tag v="ind"/>        
						              <clip pos="1" side="tl" part="lemq"/>
						            </lu>
						          </chunk>
	 						</out>
        	</when>

	        <when>
	           <test>
	             <equal>
	               <clip pos="1" side="sl" part="temps"/>
	               <lit-tag v="past"/>
	             </equal>
	           </test>
		     				<out>
						         <chunk name="VERB_PASSIVE_PAST" case="caseFirstWord">    
						           <tags>
						             <tag><lit-tag v="VERB_PASS_PAST"/></tag>     <!-- 3p sg by default-->
						              <tag><lit-tag v="vblex"/></tag>   
						              <tag><lit-tag v="actv"/></tag>   
						              <tag><lit-tag v="past"/></tag>   
						              <tag><lit-tag v="p3"/></tag>   
						              <tag><lit-tag v="sg"/></tag>   
						            </tags>
						            <lu>
						              <lit v="verða"/>
						              <lit-tag v="vblex"/>
						              <lit-tag v="actv"/>
						              <lit-tag v="past"/>
						              <lit-tag v="p3"/>
						              <lit-tag v="sg"/>
						            </lu>
						            <b pos="0"/>
						            <lu>
						              <clip pos="1" side="tl" part="lemh"/>
						              <lit-tag v="vblex"/>
						              <lit-tag v="actv"/>
									  <lit-tag v="pp"/>
				    				  <lit-tag v="m"/>
				    				  <lit-tag v="sg"/>
				    				  <lit-tag v="nom"/>
						              <lit-tag v="ind"/>        
						              <clip pos="1" side="tl" part="lemq"/>
						            </lu>
						          </chunk>
	 						</out>
        	</when>

	  </choose>
  </action>
</rule>



