    <def-macro n="conv_temps1" npar="1">	<!-- MACRO: convert present indicative to present indicative-->
      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="pres"/></equal>
          </test>
