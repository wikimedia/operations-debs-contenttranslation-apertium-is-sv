  <section-def-macros>
	
    <def-macro n="conv_case_to_acc" npar="1" > 	<!-- MACRO: convert nom to acc -->	
      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="nom"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="cas"/>
            <lit-tag v="acc"/>
          </let>
        </when>
      </choose>
    </def-macro>

    <def-macro n="conv_case_to_dat" npar="1" > 		<!-- MACRO: convert nom to dat-->	
      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="nom"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="cas"/>
            <lit-tag v="acc"/>
          </let>
        </when>
      </choose>
    </def-macro>

    <def-macro n="conv_case_to_gen" npar="1" > 		<!-- MACRO: convert nom to gen-->	
      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="nom"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="cas"/>
            <lit-tag v="gen"/>
          </let>
        </when>
      </choose>
    </def-macro>

	<def-macro n="conv_case_for_personal_pronouns" npar="1" > 						<!-- MACRO: subject/object -->	
	  <choose>
	    <when>
	      <test>
	          <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="subj"/></equal>
	      </test>
	      <let>
	        <clip pos="1" side="tl" part="cas"/>
	        <lit-tag v="nom"/>
	      </let>
	    </when>
	    <when>
	      <test>
	          <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="obj"/></equal>
	      </test>
	      <let>
	        <clip pos="1" side="tl" part="cas"/>
	        <lit-tag v="acc"/>
	      </let>
	    </when>
	  </choose>
	</def-macro>


    <def-macro n="conv_gender_temp" npar="1" > 	<!-- MACRO: used for adjectives (pst.pl, comp, sup: mfn) !!! -->	
      <choose>
        <when>
          <test>
				  <or>
	                <equal><clip pos="1" side="sl" part="gen"/><lit-tag v="ut"/></equal>
	                <equal><clip pos="1" side="sl" part="gen"/><lit-tag v="mfn"/></equal>
				  </or>
          </test>
          <let>
            <clip pos="1" side="tl" part="gen"/>
            <lit-tag v="m"/>
          </let>
        </when>
	 </choose>
    </def-macro>

							<!-- ut -> m -->	
    <def-macro n="conv_gender_for_numerals" npar="1" > 	
      <choose>
        <when>
          <test>
	                <equal><clip pos="1" side="sl" part="gen"/><lit-tag v="ut"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="gen"/>
            <lit-tag v="m"/>
          </let>
        </when>
	 </choose>
    </def-macro>    


    <def-macro n="conv_number2" npar="1" > 	<!-- MACRO: used for adjectives !!! -->	
      <choose>
        <when>
          <test>
	              <equal><clip pos="1" side="sl" part="nbr"/><lit-tag v="sp"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="nbr"/>
