  <section-rules>

    <!-- Sintagmas nominales -->




	<rule comment="REGLA: MER ADJ  | mer hyfsad → þokkalegri">   <!-- --> 
		     <pattern>
		       <pattern-item n="mer"/>
		       <pattern-item n="adj"/>
		     </pattern>
		     <action>
		       <call-macro n="firstWord">
		         <with-param pos="2"/>
		       </call-macro>
		       <call-macro n="conv_gender_temp">
		         <with-param pos="2"/>
		       </call-macro>
		       <call-macro n="conv_number2">
		         <with-param pos="2"/>
